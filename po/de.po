#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="Hakoi Pirates Infiltration">
#. <flags>
#. <unique />
#. </flags>
#. <avail>
#. <priority>10</priority>
#. <cond>
#. faction.playerStanding("Empire") &gt;= 10
#. and faction.playerStanding("Pirate") &lt; 0
#. </cond>
