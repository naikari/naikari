msgstr ""

#: dat/assets/virtual_frontier_unpresence.xml:2
msgid "Virtual Frontier Unpresence"
msgstr ""

#: dat/assets/virtual_miner_local.xml:2
msgid "Virtual Miner Local"
msgstr ""

#: dat/assets/virtual_miner_small.xml:2
msgid "Virtual Miner Small"
msgstr ""

#: dat/assets/virtual_pirate_medium.xml:2
msgid "Virtual Pirate Medium"
msgstr ""

#: dat/assets/virtual_pirate_small.xml:2
